<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000df6a-d82f-8000-9ba2-011c48011c48_112561</versionUuid>
    <rule>
        <name>CTMS_AddressView</name>
        <uuid>_a-0000ddf1-7abf-8000-daab-640000640000_21221</uuid>
        <description>CTMS list of investigator addresses </description>
        <parentUuid>_a-0000dcd5-e7b4-8000-daab-640000640000_6353</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_listViewItem"(
  image: if(
    #"_a-0000ddcb-815e-8000-daab-640000640000_18403"(
      rf!siteIcon
    ),
    todocument(
      #"_a-0000dd4f-0404-8000-daab-640000640000_16757"
    ),
    todocument(
      rf!siteIcon
    )
  ),
  title: "Site: " &amp; rf!siteIdentifier &amp; " | " &amp; "Name: " &amp; rf!accountName,
  details: rf!addressType &amp; " Address: " &amp; rf!addressLine1 &amp; ", " &amp; rf!city &amp; ", " &amp; rf!state &amp; " " &amp; rf!postalCode &amp; " | " 
          &amp; " Primary: " &amp; if(
            rf!primary == "Y",
              "Yes",
              "No"
            )
)</definition>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
    <history>
        <historyInfo versionUuid="_a-0000df6a-d82f-8000-9ba2-011c48011c48_112561"/>
    </history>
</contentHaul>
