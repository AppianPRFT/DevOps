<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000df6a-d82f-8000-9ba2-011c48011c48_112497</versionUuid>
    <rule>
        <name>CTMS_Protocol_FacetOption</name>
        <uuid>_a-0000dd4f-0404-8000-daab-640000640000_16722</uuid>
        <description>Facet Option used in the Protocol Site List Record for dynamically creating a list of protocol for User Filter</description>
        <parentUuid>_a-0000dcd5-e7b4-8000-daab-640000640000_6355</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_facetOption"(
  id: ri!id,
  name: ri!protocolIdentifier,
  filter: #"SYSTEM_SYSRULES_queryFilter"(
    field: "protocolIdentifier",
    operator: "=",
    value: ri!protocolIdentifier
  )
  /*,*/
  /*dataCount: ri!count*/
)</definition>
        <namedTypedValue>
            <name>protocolIdentifier</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>id</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>count</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
