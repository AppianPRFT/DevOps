<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000df6a-d82f-8000-9ba2-011c48011c48_112308</versionUuid>
    <rule>
        <name>GLOBAL_UserFirstLastName</name>
        <uuid>_a-0000dd3a-dada-8000-daab-640000640000_11313</uuid>
        <description>Takes a user object and returns the users first and last name separated by a space. IE. "John Doe"</description>
        <parentUuid>_a-0000dd3a-dada-8000-daab-640000640000_11112</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000dd3a-dada-8000-daab-640000640000_11122"(
    input: ri!user
  ),
  "",
  user(
    ri!user,
    "firstName"
  ) &amp; " " &amp; user(
    ri!user,
    "lastName"
  )
)</definition>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
