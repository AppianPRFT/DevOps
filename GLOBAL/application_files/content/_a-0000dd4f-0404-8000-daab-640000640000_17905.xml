<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000df6a-d82f-8000-9ba2-011c48011c48_112353</versionUuid>
    <rule>
        <name>GLOBAL_ToYesNo</name>
        <uuid>_a-0000dd4f-0404-8000-daab-640000640000_17905</uuid>
        <description>Takes a boolean and returns Yes or No instead of true or false. Null is returned if bool is null</description>
        <parentUuid>_a-0000dd3a-dada-8000-daab-640000640000_11112</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000dd3a-dada-8000-daab-640000640000_11122"(
    ri!bool
  ),
  null,
  if(
    ri!bool = true,
    "Yes",
    "No"
  )
)</definition>
        <namedTypedValue>
            <name>bool</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
