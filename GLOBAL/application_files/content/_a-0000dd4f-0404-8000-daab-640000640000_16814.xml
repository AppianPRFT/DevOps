    <rule>
        <name>GLOBAL_ReturnPercentageFromList</name>
        <uuid>_a-0000dd4f-0404-8000-daab-640000640000_16814</uuid>
        <description>Used for taking a list of values, and breaking down the amount of values a specified value from that list</description>
        <parentUuid>_a-0000dd3a-dada-8000-daab-640000640000_11112</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>count(
  wherecontains(
    ri!value,
    ri!list
  )
) / length(
  ri!list
)%</definition>
        <namedTypedValue>
            <name>value</name>
            <type>
