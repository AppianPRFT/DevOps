    <rule>
        <name>GLOBAL_DocumentImagePreview</name>
        <uuid>_a-0000dd3a-dada-8000-daab-640000640000_11202</uuid>
        <description>Used to apply to create a column in a grid for previewing data. Example usage:
a!gridImageColumn(
  label: "",
  field: "fieldname",
  data: apply(
    rule!CTMS_DocumentImagePreview(
      ri!selectedId,
      _
    ),
    local!datasubset.data.fieldname
  ),
  size: "ICON"
)</description>
        <parentUuid>_a-0000dd3a-dada-8000-daab-640000640000_11119</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_documentImage"(
  document: #"SYSTEM_SYSRULES_iconIndicator"(
    icon: "PREVIEW"
  ),
  altText: "Preview",
  link: #"SYSTEM_SYSRULES_dynamicLink"(
    value: ri!inputValue,
    saveInto: ri!saveInto
  )
)</definition>
        <namedTypedValue>
            <name>saveInto</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>inputValue</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
