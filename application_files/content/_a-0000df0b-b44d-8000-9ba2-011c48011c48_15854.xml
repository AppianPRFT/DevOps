<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000df4f-3e48-8000-9ba2-011c48011c48_60642</versionUuid>
    <rule>
        <name>ASC_ArrayPickerFilter</name>
        <uuid>_a-0000df0b-b44d-8000-9ba2-011c48011c48_15854</uuid>
        <description>Scans labels that match the text entered by the user and returns a DataSubset for use in the SAIL picker component.</description>
        <parentUuid>_a-0000deba-19b3-8000-9ba2-011c48011c48_32853</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(
  local!matches: where(
    #"SYSTEM_SYSRULES_forEach"(
      items: ri!labels, 
      expression: search( ri!filter, fv!item)
    )
  ),
  #"SYSTEM_SYSRULES_dataSubset"(
    data: index( ri!labels, local!matches), 
    identifiers: index( ri!identifiers, local!matches)
  )
)</definition>
        <namedTypedValue>
            <name>filter</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>labels</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>identifiers</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
